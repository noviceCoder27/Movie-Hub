{
    "version":2,
    "builds": [
        { "src": "dist/*.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "/",
          "methods":["GET","PATCH","POST", "DELETE", "PUT"],
          "headers": {
            "Access-Control-Allow-Origin": "*"
          }
        }
    ]
    
}